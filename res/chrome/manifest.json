{
    "name": "::FUbar",
    "version": "::version::",
    "description": "Animated GIF Television",
    "manifest_version": 2,
    "minimum_chrome_version": "49",
    "offline_enabled": false,
    "short_name": "FUbar",
    "app": {
        "background": { "scripts": ["background.js"] }
    },
    "permissions": [
        "clipboardWrite",
        "http://*/*",
        "https://api.giphy.com/*",
        "storage"
    ],
    "icons": {
        "16": "icon/fubar-16.png",
        "48": "icon/fubar-48.png",
        "128": "icon/fubar-128.png"
    }
}
